<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="mainWindow" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="C195.LoginController">
   <children>
      <TextField fx:id="username" layoutX="220.0" layoutY="136.0" />
      <TextField fx:id="password" layoutX="220.0" layoutY="177.0" />
      <Button fx:id="exit" layoutX="343.0" layoutY="288.0" mnemonicParsing="false" onAction="#exit" text="Exit" />
      <Button fx:id="login" layoutX="272.0" layoutY="212.0" mnemonicParsing="false" onAction="#login" text="Login" />
      <Label fx:id="usernameLabel" layoutX="120.0" layoutY="140.0" text="Username" />
      <Label fx:id="passwordLabel" layoutX="122.0" layoutY="181.0" text="Password" />
      <Label fx:id="locationLabel" layoutX="369.0" layoutY="333.0" text="Location:" />
      <Label fx:id="setLocation" layoutX="464.0" layoutY="333.0" text="Label" />
   </children>
</AnchorPane>
